  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@types/jest": "24.0.19",
    "@types/node": "^12.7.12",
    "@types/react": "16.9.6",
    "@types/react-dom": "16.9.2",
    "@types/styled-components": "^4.1.19",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "react": "^16.10.2",
